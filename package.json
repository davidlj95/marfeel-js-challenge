  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidlj95/marfeel-js-challenge/issues"
  },
  "homepage": "https://github.com/davidlj95/marfeel-js-challenge#readme",
  "dependencies": {
    "d3": "^5.15.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.0.0",
    "browser-sync": "^2.26.7",
    "concurrently": "^5.1.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-jasmine": "^3.1.0",
