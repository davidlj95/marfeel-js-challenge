  "description": "Marfeel vanilla JS technical test",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "gitignore": "curl -sq http://gitignore.io/api/vim,node,linux,macos,windows,webstorm+iml > .gitignore && echo build/ >> .gitignore",
